{
  "compilerOptions": {
    "jsx": "react",
    "target": "ESNext",
    "outDir": "./dist/",
    "moduleResolution": "Node",
    "module": "ESNext",
    "allowSyntheticDefaultImports": true,
    "baseUrl": ".",
    "paths": {
      "@actions": [
        "./src/actions/index"
      ],
      "@constants/*": [
        "./src/constants/*"
      ],
      "@reducers/*": [
        "./src/reducers/*"
      ],
      "@shared/*": [
        "./src/shared/*"
      ],
      "@store": [
        "./src/store/index"
      ],
      "@utils/*": [
        "./src/utils/*"
      ],
      "@views/*": [
        "./src/views/*"
      ]
    }
  }
}