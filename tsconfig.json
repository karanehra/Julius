{
  "compilerOptions": {
    "jsx": "react",
    "target": "ESNext",
    "outDir": "./dist/",
    "moduleResolution": "Node",
    "module": "ESNext",
    "allowSyntheticDefaultImports": true,
    "baseUrl": ".",
    "paths": {
      "@constants/*": [
        "./src/constants/*"
      ],
      "@utils/*": [
        "./src/utils/*"
      ]
    }
  }
}